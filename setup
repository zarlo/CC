write( "Connecting to server... " )
local response = http.get(
        "https://raw.githubusercontent.com/zarlo/CC/master/nget"
)
        
if response then       
    local sResponse = response.readAll()
    response.close()
    local file = fs.open( "nget", "w" )
    file.write( sResponse )
    file.close()
else
    printError( "Failed." )
    return
end

shell.run("nget", "get", "update", "update")
shell.run("nget", "run", "getAPI")


shell.run("pastebin", "get", "w1RtfqFd", "apps/market")
	
	
os.reboot()