
os.loadApi("tst")


--run user auto run
if fs.exists( "/autorun" ) and fs.isDir( "/autorun" ) then
	local tFiles = fs.list( "/autorun" )
	table.sort( tFiles )
	for n, sFile in ipairs( tFiles ) do
		if string.sub( sFile, 1, 1 ) ~= "." then
			local sPath = "/autorun/"..sFile
			if not fs.isDir( sPath ) then
				shell.run( sPath )
			end
		end
	end
end